 // calculate the sum of 5 sujbesct and find the percentage
#include<stdio.h>
void main()
{
    int s1,s2,s3,s4,s5,sum,total=500;
    float per;
    printf("enter the marks of five subject/n");
    scanf("%d%d%d%d%d",&s1,&s2,&s3,&s4,&s5);
    sum=s1+s2+s3+s4+s5;
    printf("sum=%d",sum);
    per=(sum*100)/total;
    printf("percentage=%f",per);
    
    
}
